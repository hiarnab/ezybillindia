{
  "extends": "airbnb-base",
  "env": {
      "es6": true,
      "browser": true
  },
  "rules": {
      "brace-style": ["error", "stroustrup"],
      "comma-dangle": ["error", "never"],
      "no-unused-vars": ["warn"],
      "no-var": ["off"],
      "one-var": ["off"],
      "import/no-extraneous-dependencies": ["warn"],
      "no-plusplus": ["off"],
      "max-len": "off",
      "no-param-reassign": "warn",
      "no-restricted-syntax": "off",
      "linebreak-style": "off"
  }
}